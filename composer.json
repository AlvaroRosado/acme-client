{
    "name": "kelunik/acme-client",
    "description": "Standalone PHP ACME client.",
    "keywords": [
        "ACME",
        "letsencrypt",
        "certificate",
        "https",
        "encryption",
        "ssl",
        "tls"
    ],
    "require": {
        "php": "^5.5|^7",
        "ext-openssl": "*",
        "amphp/process": "^0.1.1",
        "kelunik/acme": "^0.3",
        "kelunik/certificate": "^1",
        "league/climate": "^3",
        "rdlowrey/auryn": "^1",
        "webmozart/assert": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5",
        "fabpot/php-cs-fixer": "^1.9",
        "macfja/phar-builder": "dev-master#a2db582eab26ef7b15144c013408749a79fae361"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Kelunik\\AcmeClient\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "extra": {
        "phar-builder": {
            "compression": "GZip",
            "name": "acme-client.phar",
            "output-dir": "build",
            "include": ["src", "vendor/kelunik/acme/res"],
            "entry-point": "bin/acme"
        }
    }
}
