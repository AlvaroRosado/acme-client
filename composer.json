{
    "name": "kelunik/acme-client",
    "description": "Standalone PHP ACME client.",
    "keywords": [
        "ACME",
        "letsencrypt",
        "certificate",
        "https",
        "encryption",
        "ssl",
        "tls"
    ],
    "require": {
        "amphp/process": "^0.1.1",
        "bramus/monolog-colored-line-formatter": "^2",
        "ext-posix": "*",
        "ext-openssl": "*",
        "kelunik/acme": "^0.3",
        "kelunik/certificate": "^1",
        "league/climate": "^3",
        "monolog/monolog": "^1.17",
        "php": "^5.5|^7",
        "psr/log": "^1",
        "rdlowrey/auryn": "^1",
        "webmozart/assert": "^1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Kelunik\\AcmeClient\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^5"
    }
}
